{
  "name": "ws-finder",
  "version": "1.0.0",
  "description": "Webservice utilizado pelo aplicativo Finder",
  "main": "index.js",
  "scripts": {
    "build": "tsc --build",
    "start": "ts-node-dev src/server.ts",
    "test": "NODE_ENV=test PORT=7788 jest",
    "test:watch": "npm run test -- --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/silvalazaro/ws-finder.git"
  },
  "author": "silvalazaro <lazarofdsilva@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/silvalazaro/ws-finder/issues"
  },
  "homepage": "https://github.com/silvalazaro/ws-finder#readme",
  "dependencies": {
    "class-transformer": "^0.2.3",
    "dotenv": "^8.2.0",
    "jsonwebtoken": "^9.0.0",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-json": "^2.0.2",
    "koa-jwt": "^4.0.0",
    "koa-logger": "^3.2.1",
    "koa-mongo": "^1.9.1",
    "koa-passport": "^4.1.3",
    "koa-redis": "^4.0.1",
    "koa-router": "^9.0.1",
    "koa-sslify": "^4.0.3",
    "koa2-cors": "^2.0.6",
    "module-alias": "^2.2.2",
    "mongodb": "^3.5.9",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "devDependencies": {
    "@types/jest": "^26.0.3",
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-router": "^7.4.1",
    "@types/koa2-cors": "^2.0.1",
    "@types/node": "^14.0.14",
    "@types/redis": "^2.8.22",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "eslint": "^7.3.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.1",
    "ts-node-dev": "^1.0.0-pre.49"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "roots": [
      "<rootDir>/__tests__"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
